{
  "name": "sometime-riot",
  "version": "0.1.0",
  "description": "Timepicker for Riot",
  "jsnext:main": "dist/sometime.es6.js",
  "main": "dist/sometime.cjs.js",
  "browser": "dist/sometime.amd.js",
  "homepage": "https://github.com/cognitom/sometime-riot",
  "repository": {
    "type": "git",
    "url": "git://github.com/cognitom/sometime-riot.git"
  },
  "author": "Tsutomu Kawamura <cognitom@gmail.com>",
  "license": "MIT",
  "keywords": [
    "riot",
    "riotcomponent",
    "component"
  ],
  "dependencies": {
    "riot": "^2.3.13",
    "riot-mixin-pack": "github:cognitom/riot-mixin-pack"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.0.0",
    "browser-sync": "^2.10.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^1.10.3",
    "expect.js": "^0.3.1",
    "karma": "^0.13.19",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.3",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "riot-compiler": "^2.3.20",
    "rollup": "^0.24.0",
    "rollup-plugin-babel": "^2.3.8",
    "rollup-plugin-commonjs": "^2.1.0",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-npm": "^1.2.1",
    "rollup-pluginutils": "^1.3.1",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "npm run eslint && npm run karma",
    "prepublish": "npm run build",
    "build": "node rollup.js",
    "watch": "chokidar 'riot/*.js' -c 'npm run build'",
    "eslint": "eslint -c ./.eslintrc *.js",
    "karma": "karma start test/karma.conf.js"
  }
}
